{
  "name": "ipa-snake-js-game",
  "version": "1.0.0",
  "description": "Adaptation of the classic game snake",
  "main": "dist/js/ipa-snake-js-game.js",
  "module": "dist/js/ipa-snake-js-game.esm.js",
  "sass": "src/scss/snake-game.scss",
  "style": "dist/css/snake-game.css",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "build": "npm run production",
    "prod": "npm run production",
    "production": "cross-env NODE_ENV=production NODE_OPTIONS=--max_old_space_size=4096 webpack",
    "dev": "npm run development",
    "development": "cross-env NODE_ENV=development NODE_OPTIONS=--max_old_space_size=4096 webpack",
    "clear-cache": "cd ./node_modules && rm -r ./.cache"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/darkdiesel/snake-js-oop.git"
  },
  "keywords": [
    "game",
    "javascript",
    "sass",
    "css",
    "snake"
  ],
  "author": "Igor Peshkov",
  "license": "GPL-3.0",
  "devDependencies": {
    "autoprefixer": "^10.4.17",
    "cross-env": "^7.0.3",
    "css-loader": "^6.9.1",
    "http-server": "^14.1.1",
    "mini-css-extract-plugin": "^2.7.7",
    "postcss": "^8.4.33",
    "postcss-cli": "^11.0.0",
    "postcss-combine-duplicated-selectors": "^10.0.3",
    "postcss-combine-media-query": "^1.0.1",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-import": "^16.0.0",
    "postcss-loader": "^8.0.0",
    "postcss-merge-rules": "^6.0.3",
    "postcss-sort-media-queries": "^5.2.0",
    "postcss-will-change": "^4.0.1",
    "sass-loader": "^14.0.0",
    "style-loader": "^3.3.4",
    "webpack": "^5.90.0",
    "webpack-cli": "^5.1.4",
    "webpack-remove-empty-scripts": "^1.0.4"
  }
}
